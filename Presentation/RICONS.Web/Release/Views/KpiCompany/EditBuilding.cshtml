        DataJson += "'khongbosung':'" + $('#txtkhongbosung').val() + "',";

        DataJson += "'truongbophan_cht':'" + $('#txttruongbophan_cht').val() + "',";
        DataJson += "'truongbophan_cht_email':'" + $('#txttruongbophan_cht_email').val() + "',";

