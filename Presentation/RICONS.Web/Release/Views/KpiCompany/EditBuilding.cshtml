        DataJson += "'phongqtnnl':'" + $('#txtphongqtnnl').val() + "',";
        DataJson += "'phongqtnnl_email':'" + $('#txtphongqtnnl_email').val() + "',";

        DataJson += "'bantonggiamdoc':'" + $('#txtbantonggiamdoc').val() + "',";
        DataJson += "'bantonggiamdoc_email':'" + $('#txtbantonggiamdoc_email').val() + "',";

        DataJson += "'phongban_congtruong':'1'";

        DataJson += "}, 'data2':[";

        $('#boxContent table tbody tr').each(function (index, item) {
            debugger;
            var matiendo = $(item).attr('matiendo');
            var mabosungnhansu = $(item).attr('mabosungnhansu');
            var hangmucthicong = $(item).find('td.col2').text();
            var giatri = $(item).find('td.col3 input').val();
            var batdau = $(item).find('td.col4 input').val();
            var ketthuc = $(item).find('td.col5 input').val();
            var duphong = $(item).find('td.col6 input').val();
            var ghichu = $(item).find('td.col7 input').val();

            DataJson += "{";
            DataJson += "'matiendo':'" + matiendo + "',";
            DataJson += "'mabosungnhansu':'" + mabosungnhansu + "',";
            DataJson += "'hangmucthicong':'" + hangmucthicong + "',";
            DataJson += "'giatri':'" + giatri + "',";
            DataJson += "'batdau':'" + batdau + "',";
            DataJson += "'ketthuc':'" + ketthuc + "',";
            DataJson += "'duphong':'" + duphong + "',";
            DataJson += "'ghichu':'" + ghichu + "'";
            DataJson += "}";
            if (index != $('#boxContent table tbody tr').length - 1)
                DataJson += ",";
        });
        DataJson += "], 'data3':["; //,'data3':[

        $('#boxContent1 table tbody tr').each(function (index, item) {
            debugger;
            var makehoach = encodeURIComponent($(item).attr('makehoach'));
            var mabosungnhansu = encodeURIComponent($(item).attr('mabosungnhansu'));

            var vitricongtac = $(item).find('td.cols2').text();

            var soluong = $(item).find('td.cols3 input').val();
            var thoigianbosung = $(item).find('td.cols4 input').val();

            var soluong1 = $(item).find('td.cols5 input').val();
            var thoigianbosung1 = $(item).find('td.cols6 input').val();

            var soluong2 = $(item).find('td.cols7 input').val();
            var thoigianbosung2 = $(item).find('td.cols8 input').val();

            var ghichu = $(item).find('td.cols9 input').val();


            DataJson += "{";
            DataJson += "'makehoach':'" + makehoach + "',";
            DataJson += "'mabosungnhansu':'" + mabosungnhansu + "',";
            DataJson += "'vitricongtac':'" + vitricongtac + "',";
            DataJson += "'soluong':'" + soluong + "',";
            DataJson += "'thoigianbosung':'" + thoigianbosung + "',";

            DataJson += "'soluong1':'" + soluong1 + "',";
            DataJson += "'thoigianbosung1':'" + thoigianbosung1 + "',";

            DataJson += "'soluong2':'" + soluong2 + "',";
            DataJson += "'thoigianbosung2':'" + thoigianbosung2 + "',";

            DataJson += "'ghichu':'" + ghichu + "'";
            DataJson += "}";
            if (index != $('#boxContent1 table tbody tr').length - 1)
                DataJson += ",";
        });
        DataJson += "]}";

        $.ajax({
            type: 'POST',
            url: '/AddStaff/Save',
            datatype: "JSON",
            data: "DataJson=" + DataJson,
            cache: false,

        })
        .done(function (result) {
            debugger;
            var tb = JSON.stringify(result.success);
            var idbosungnhansu = JSON.stringify(result.mabosungnhansu);
            if (tb == "true") {
                var dialogSuccess = bootbox.dialog({
                    message: "<i class='fa fa-check text-success'></i><span class='text-success'>Lưu dữ liệu thành công.</span>",
                    closeButton: false,
                    onEscape: function () {
                    }
                });
                setTimeout(function () {
                    $('#txtmabosungnhansu').val(idbosungnhansu);
                    dialogSuccess.modal('hide');
                }, 2000);
            } else {
                var dialogSuccess = bootbox.dialog({
                    message: "<i class='fa fa-check text-success'></i><span class='text-success'>Lưu dữ liệu không thành công</span>",
                    closeButton: false,
                    onEscape: function () {
                    }
                });
                setTimeout(function () {
                    $('#txtmabosungnhansu').val(0);
                    dialogSuccess.modal('hide');
                }, 2000);
            }
        })
        .fail(function (result) {
            alert("FAILED: " + JSON.stringify(result));

        });
    })

