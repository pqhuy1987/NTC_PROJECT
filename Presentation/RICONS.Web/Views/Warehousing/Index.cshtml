<div class="HeaderFixed">
    <div class="b-b b-light">
        <div class="row" style="color:black; line-height:30px;">
            <div class=" col-md-3" style="font-size: 16px; font-weight: bold; line-height: 31px;width:200px;">DS phiếu nhập kho</div>
            <div class="col-md-4" style="width:320px">
                <a href="@Url.Action("New_Warehousing", "Warehousing")" class="buttoncustom WhiteButton">Thêm mới</a>
                <a id="btnHieuChinh" href="@Url.Action("Edit_Warehousing", "Warehousing")" class="buttoncustom WhiteButton">Hiệu chỉnh</a>
                <a id="btnDeleted" href="@Url.Action("Deleted", "Warehousing")" class="buttoncustom WhiteButton">Xoá</a>
            </div>
            <div class="col-md-1" style="width:70px;">Từ ngày:</div>
            <div class="col-md-1 boxEdit">
                <input type="text" style="width:100%" autocomplete="off" placeholder="Từ ngày" id="txttungayct" name="ngaychungtu" />
            </div>
            <div class="col-md-1" style="width:75px;">Đến ngày:</div>

            <div class="col-md-1 boxEdit">
                <input type="text" style="width:100%" autocomplete="off" placeholder="Đến ngày" id="txtdenngayct" name="denngaychungtu" />
            </div>
            <div class="col-md-2">
                Export:&nbsp; <a href="@Url.Action("ExportLicensing", "Absent")"><i class="fa fa-file-excel-o" style="color:black;"></i> Excel</a>
                </div>



            </div>

        </div>
    <div class="clearBoth"></div>
</div>


<div style="width:100%; height:75%;">
    <div id="boxContent">
        <div class="row" style="padding-right:5px">
            <div class="col-md-12">
                <div class="portlet-body">
                    <div class="table-responsive">
                        <table class="table table-bordered table_danhSach" id="tableContent">
                            <thead>
                            </thead>
                            <tbody></tbody>
                            <tfoot>
                                <tr>
                                    <td colspan="9" id="girdInfo">Tổng tiển: <span class="total">0</span> - Tổng số: <span class="currentRow">0</span>/<span class="sumRow">0</span></td>
                                </tr>
                                <tr>
                                    <td colspan="16" id="girdInfoLoad">
                                        <div class="divExentd"><i class="retweet_32"></i>Xem thêm</div>
                                    </td>
                                </tr>
                                @*<tr>
